{
    "name": "app-insights-php/client",
    "description": "Microsoft App Insights telemetry client php wrapper",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
